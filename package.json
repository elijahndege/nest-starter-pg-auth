{
  "name": "nest-ts-starter",
  "private": true,
  "version": "1.0.0",
  "description": "Nest TypeScript starter with static testing",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "check-types": "tsc --incremental false --noEmit",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "prettier": "prettier --parser typescript --ignore-path .gitignore \"src/**/*.{js,json,ts,tsx}\"",
    "check-format": "npm run prettier -- --list-different",
    "validate": "npm-run-all --parallel test:cov check-format lint"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@nestjs/common": "^7.0.13",
    "@nestjs/config": "^0.4.2",
    "@nestjs/core": "^7.0.13",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^7.0.13",
    "@nestjs/typeorm": "^7.0.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "express-rate-limit": "^5.1.3",
    "helmet": "^3.22.0",
    "nest-emitter": "^1.1.0",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.2.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.5",
    "typeorm": "^0.2.24",
    "uuid": "^8.0.0",
    "validator": "^13.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.1.5",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.13",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.6",
    "@types/express-rate-limit": "^5.0.0",
    "@types/hapi__joi": "^17.1.0",
    "@types/helmet": "0.0.47",
    "@types/jest": "^25.2.3",
    "@types/jest-in-case": "^1.0.2",
    "@types/node": "^14.0.1",
    "@types/passport-google-oauth20": "^2.0.3",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.9",
    "@types/uuid": "^7.0.3",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "jest-in-case": "^1.0.2",
    "jest-watch-select-projects": "^2.0.0",
    "jest-watch-typeahead": "^0.6.0",
    "lint-staged": "^10.2.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "^26.0.0",
    "ts-loader": "^7.0.4",
    "ts-node": "^8.10.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.2"
  }
}
